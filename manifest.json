{
    "name": "JAO: Job Application Organizer",
    "version": "1.0",
    "description": "To help jobseekers organize the process!",
    "manifest_version": 2,
    "permissions": [
      "storage", "tabs"
    ],
    "chrome_url_overrides": {
      "newtab": "home.html"
    },
    "browser_action": {
      "default_icon": "icon.png",
      "default_title": "Quick Insert",      
      "default_popup": "popup.html"       
    }
  }